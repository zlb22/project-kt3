# App
APP_TITLE=keti3 Python API
APP_VERSION=0.1.0
APP_PORT=8096

# Signature
APP_ID=keti-3
SKEY=please-change-me
# Optional timestamp drift seconds (0 to disable)
SIGN_TS_DRIFT=0

# JWT (must match backend/main.py if integrating existing JWT)
SECRET_KEY=your-secret-key-change-in-production
ALGORITHM=HS256

# Redis（可选；留空或注释代表禁用 Redis 回退，仅使用 JWT）
# REDIS_URL=redis://localhost:6379/0

# MySQL (will be used when wiring SQLAlchemy)
MYSQL_DSN=mysql+pymysql://user:pass@localhost:3306/base_keti?charset=utf8mb4

# MinIO
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_USE_SSL=false
MINIO_BUCKET=onlineclass
MINIO_BASE_PATH=24game
# Optional public base (e.g., https://files.example.com/onlineclass)
# MINIO_PUBLIC_BASE=
